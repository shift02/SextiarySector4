        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true",
        "west": "false"
      },
      "apply": {
        "model": "sextiarysector4:block/leaf_bed_leaf_west",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "false",
